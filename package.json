{
  "name": "x-team-2023",
  "author": "X-Team",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rimraf out .next && next build",
    "start": "npx serve@latest out",
    "lint": "eslint --no-ignore .eslintrc.js next.config.js package.json jest.config.ts \"src/**/*.{js,jsx,ts,tsx,json}\" --format=pretty",
    "lint-fix": "eslint .eslintrc.js next.config.js package.json jest.config.ts \"src/**/*.{js,jsx,ts,tsx,json}\" --no-ignore --fix --format=pretty",
    "format": "prettier --write .eslintrc.js next.config.js package.json jest.config.ts \"src/**/*.{js,jsx,ts,tsx,json,css}\" && npm run lint-fix",
    "type-check": "tsc --pretty --noEmit",
    "deploy": "rimraf out && mkdir out && touch out/.nojekyll && storybook build -o ./out && gh-pages -d out -t true",
    "docs": "storybook dev -p 6008",
    "build-docs": "storybook build -o ./out/docs",
    "gen": "ts-node ./dev/generator/index.ts",
    "test": "jest",
    "test:watch": "npm run jest -- --watch",
    "test:coverage": "npm run jest -- --coverage",
    "test:coverage:watch": "npm run jest -- --coverage --watch",
    "clear-cache": "rimraf out .next"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'npm run type-check'"
    ],
    "!(static/**/*|dev/generator/templates/**/*).{js,jsx,ts,tsx}": [
      "bash -c 'npm run format'"
    ]
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.40",
    "axios": "^1.5.1",
    "framer-motion": "^10.16.2",
    "next": "13.4.19",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "18.2.0",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-masonry-css": "^1.0.16",
    "react-select": "^5.7.7",
    "react-slideshow-image": "^4.2.1",
    "react-snowfall": "^1.2.1",
    "sharp": "^0.32.5",
    "tailwindcss": "3.3.3",
    "typeface-inter": "^3.18.1",
    "universal-cookie": "^6.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@faker-js/faker": "^8.0.2",
    "@inquirer/prompts": "^3.1.2",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/figlet": "^1.5.6",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "20.5.7",
    "@types/react": "^18.2.24",
    "@types/react-dom": "18.2.7",
    "@types/react-mailchimp-subscribe": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "10.4.15",
    "babel-jest": "^29.7.0",
    "chalk": "4.1.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.13",
    "figlet": "^1.6.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-search-list": "^1.2.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "storybook": "^7.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2"
  }
}
